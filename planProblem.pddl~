(define (problem elevators-sequencedstrips-problem)
	(:domain elevators-sequencedstrips)
	(:objects a b c d e a aa b)
	(:init
		(above a)
		(above b)
		(next c e)
		(next d d)
		(next e a)
	)
	(:goal (exists (?x ?y)
		(and 
			(mko (above ?x))
			(mko (next ?x ?y))
			(not (mko-eq ?x ?y))
		)
	))
)
